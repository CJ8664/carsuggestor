<div class="row">
  <div class="col s9">
    <p>{{car}}</p>
  </div>
  <div class="col s3">
    <form class="col s12" action="/save" method="post">
      <div class="row">
        <div class="input-field col s12">
          <h5>Are you happy with the result?</h5>
          <p>
            <label>
            <input class="with-gap" name="happy" type="radio" value="yes"/>
            <span>Yes</span>
          </label>
          </p>
          <p>
            <label>
            <input class="with-gap" name="happy" type="radio" value="no"/>
            <span>No</span>
          </label>
          </p>
        </div>
      </div>
      <div class="row">
        <div class="col s12">
          <button class="btn waves-effect waves-light" type="submit" name="action">
        Submit<i class="material-icons right">send</i>
        </button>
        </div>
      </div>
    </form>
  </div>
</div>
<div class="row">
  <div class="col s6">
    <ul class="collection">
      <a class="collection-item active"><h5>Positive Reviews</h5></a>
      {{#each positive}}
        <li class="collection-item">{{this}}</li>
      {{/each}}
    </ul>
  </div>
  <div class="col s6">
    <ul class="collection">
      <a class="collection-item active"><h5>Negative Reviews</h5></a>
      {{#each negative}}
        <li class="collection-item">{{this}}</li>
      {{/each}}
    </ul>
  </div>
</div>
