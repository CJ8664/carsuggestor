<div class="row">
  <h5>Are you happy with the result?</h5>
  <form class="col s12" action="/save" method="post">
    <div class="input-field col s4">
      <p>
        <label>
        <input class="with-gap" name="happy" type="radio" value="yes"/>
        <span>Yes</span>
      </label>
      </p>
    </div>
    <div class="input-field col s4">
      <p>
        <label>
        <input class="with-gap" name="happy" type="radio" value="no"/>
        <span>No</span>
      </label>
      </p>
    </div>
    <div class="col s4">
      <button class="btn waves-effect waves-light" type="submit" name="action">
        Submit<i class="material-icons right">send</i>
      </button>
    </div>
  </form>
</div>
{{#each cars}}
  <div class="row">
    <h6><b>{{this.car}}</b> | <a href="http://images.google.com/images?q={{this.car}}" target="_blank">Show Photos</a></h6>
    <div class="col s6">
      <ul class="collection">
        <a class="collection-item active"><h5>Positive Reviews</h5></a>
        {{#each this.positive}}
          <li class="collection-item">{{this}}</li>
        {{/each}}
      </ul>
    </div>
    <div class="col s6">
      <ul class="collection">
        <a class="collection-item active"><h5>Negative Reviews</h5></a>
        {{#each this.negative}}
          <li class="collection-item">{{this}}</li>
        {{/each}}
      </ul>
    </div>
  </div>
{{/each}}
